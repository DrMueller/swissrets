  </xs:simpleType>
  <xs:simpleType name="urlTypes">
    <xs:annotation>
      <xs:documentation>Url types define the usage and implementation of a url. It should not describe the url itself.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="link" />
      <!-- a genneric link for display and hrefing. -->
      <xs:enumeration value="direct-link" />
      <!-- use as replacement link. Aka. a custom Endpoint webpage instead of a generated single/data view. -->
      <xs:enumeration value="virtual-tour" />
      <!-- to embed specificaly (usualy iframe) -->
      <xs:enumeration value="youtube" />
      <!-- to embed specificaly with youtube api -->
      <xs:enumeration value="archilogic" />
      <!-- to embed specificaly with archilogic api -->
      <xs:enumeration value="emonitor" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="utilityTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="commercial" />
      <xs:enumeration value="gastronomy" />
      <xs:enumeration value="vacation" />
      <xs:enumeration value="agricultural" />
      <xs:enumeration value="industrial" />
      <xs:enumeration value="residential" />
      <xs:enumeration value="storage" />
      <xs:enumeration value="parking" />
      <xs:enumeration value="building" />
      <xs:enumeration value="investment" />
    </xs:restriction>
  </xs:simpleType>
  <!-- Todo: Mimetypes should be defined -->
  <xs:simpleType name="mediaTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="image" />
      <xs:enumeration value="document" />
      <xs:enumeration value="plan" />
      <xs:enumeration value="offer-logo" />
      <xs:enumeration value="sales-brochure" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="emailAddressType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[^@]+@[^\.]+\..+" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="export">
    <xs:complexType>
      <xs:all>
        <xs:element name="exportInformation" maxOccurs="1" minOccurs="0">
          <xs:complexType>
            <xs:all>
              <xs:element type="xs:string" name="software" minOccurs="0" maxOccurs="1" />
              <xs:element name="softwareVersion" type="xs:string" minOccurs="0" maxOccurs="1" />
              <xs:element type="xs:dateTime" name="exportDate" minOccurs="0" maxOccurs="1" />
              <xs:element type="xs:integer" name="exportIteration" default="1" minOccurs="0" maxOccurs="1" />
            </xs:all>
          </xs:complexType>
        </xs:element>
        <xs:element name="projects" maxOccurs="1" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="project" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:all>
                    <xs:element type="xs:string" name="referenceId" minOccurs="1" maxOccurs="1" />
                    <xs:element name="units" maxOccurs="1" minOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="unit" maxOccurs="unbounded" minOccurs="1">
                            <xs:complexType>
                              <xs:all>
                                <xs:element type="xs:string" name="referenceId" minOccurs="1" maxOccurs="1" />
                                <xs:element name="properties" maxOccurs="1" minOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <!-- <xs:element name="propertyRef" maxOccurs="unbounded" minOccurs="1" type="xs:IDREF" /> -->
                                      <xs:element maxOccurs="unbounded" minOccurs="1" type="xs:string" name="propertyRef" />
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:all>
                              <xs:attribute type="xs:ID" name="id" use="optional" />
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                  <xs:attribute type="xs:ID" name="id" use="optional" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="properties" maxOccurs="1" minOccurs="0">
          <xs:annotation>
            <xs:documentation />
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:annotation>
                <xs:documentation>Order of properties is relevant. Custom sorting can be defined by provider.
                </xs:documentation>
              </xs:annotation>
              <xs:element name="property" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:all>
                    <xs:element type="xs:string" name="zoneTypes" minOccurs="0" maxOccurs="1" />
                    <xs:element type="xs:string" name="parcelNumbers" minOccurs="0" maxOccurs="1" />
                    <xs:element type="xs:string" name="referenceId" minOccurs="1" maxOccurs="1">
                      <xs:annotation>
                        <xs:documentation> Can be used to track the original creation id (this should usualy be unique to the property)
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element type="xs:string" name="visualReferenceId" minOccurs="0" maxOccurs="1">
                      <xs:annotation>
                        <xs:documentation> Can should only be used for presentational purposes publishers tend to present the referenceId if
                          this one is missing
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="type" type="propertyType" minOccurs="1" maxOccurs="1" />
                    <xs:element name="availability" type="availabilityType" minOccurs="1" maxOccurs="1" />
                    <xs:element name="start" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
                    <xs:element name="prices" type="pricesType" minOccurs="0" maxOccurs="1" />
                    <xs:element name="address" type="addressType" minOccurs="1" maxOccurs="1" />
                    <xs:element name="softwareInformation" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:all>
                          <xs:element type="xs:string" name="lastUser" minOccurs="0" maxOccurs="1" />
                          <xs:element type="xs:dateTime" name="lastUpdate" minOccurs="0" maxOccurs="1" />
                          <xs:element type="xs:byte" name="updateIteration" minOccurs="0" maxOccurs="1" />
                          <xs:element type="xs:dateTime" name="creation" minOccurs="0" maxOccurs="1" />
                        </xs:all>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="characteristics" type="characteristicsType" minOccurs="0" maxOccurs="1" />
                    <xs:element name="categories" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:annotation>
                            <xs:documentation> Order represents (least specific) to (most specific). For example house, single-house,
                              rustico
                            </xs:documentation>
                          </xs:annotation>
                          <xs:element type="categoryTypes" name="category" minOccurs="1" maxOccurs="unbounded" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="utilities" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="utilityTypes" name="utility" minOccurs="1" maxOccurs="3" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="seller" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:all>
                          <xs:element name="organization" minOccurs="0" maxOccurs="1">
                            <xs:complexType>
                              <xs:all>
                                <xs:element name="address" type="addressType" minOccurs="0" maxOccurs="1" />
                                <xs:element type="xs:string" name="legalName" minOccurs="0" maxOccurs="1" />
                                <xs:element type="xs:string" name="brand" minOccurs="0" maxOccurs="1" />
                                <xs:element type="emailAddressType" name="email" minOccurs="0" maxOccurs="1" />
                                <xs:element type="emailAddressType" name="emailRem" minOccurs="0" maxOccurs="1" />
                                <xs:element type="xs:string" name="fax" minOccurs="0" maxOccurs="1" />
                                <xs:element type="xs:string" name="phone" minOccurs="0" maxOccurs="1" />
                                <xs:element name="website" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:simpleContent>
                                      <xs:extension base="xs:anyURI">
                                        <xs:attribute type="xs:string" name="title" />
                                        <xs:attribute type="xs:string" name="label" />
                                      </xs:extension>
                                    </xs:simpleContent>
                                  </xs:complexType>
                                </xs:element>
                              </xs:all>
                              <xs:attribute type="xs:string" name="id" use="optional" />
                            </xs:complexType>
                          </xs:element>
                          <xs:element type="personType" name="viewPerson" minOccurs="0" maxOccurs="1" />
                          <xs:element type="personType" name="visitPerson" minOccurs="0" maxOccurs="1" />
                          <xs:element type="personType" name="inquiryPerson" minOccurs="0" maxOccurs="1" />
                        </xs:all>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="offers" minOccurs="1" maxOccurs="1">
                      <xs:annotation>
                        <xs:documentation />
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:annotation>
                            <xs:documentation> Order is relevant. First offer is considered to be the main language etc.
                            </xs:documentation>
                          </xs:annotation>
                          <xs:element name="offer" minOccurs="1" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:all>
                                <xs:element type="xs:string" name="name" minOccurs="1" maxOccurs="1" />
                                <xs:element type="xs:string" name="excerpt" minOccurs="0" maxOccurs="1" />
                                <xs:element name="tags" maxOccurs="1" minOccurs="0">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="tag" minOccurs="0" maxOccurs="unbounded" />
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="urls" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element type="fullUrlType" name="url" minOccurs="0" maxOccurs="unbounded" />
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="publishers" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="publisher" minOccurs="1" maxOccurs="unbounded">
                                        <xs:complexType>
                                          <xs:all>
                                            <xs:element name="options" minOccurs="0" maxOccurs="1">
                                              <xs:complexType mixed="true">
                                                <xs:sequence>
                                                  <xs:element name="option" maxOccurs="unbounded" minOccurs="1">
                                                    <xs:complexType>
                                                      <xs:simpleContent>
                                                        <xs:extension base="xs:string">
                                                          <xs:attribute type="xs:string" name="key" use="optional" />
                                                        </xs:extension>
                                                      </xs:simpleContent>
                                                    </xs:complexType>
                                                  </xs:element>
                                                </xs:sequence>
                                              </xs:complexType>
                                            </xs:element>
                                            <xs:element name="channels" minOccurs="0" maxOccurs="1">
                                              <xs:complexType mixed="true">
                                                <xs:sequence>
                                                  <xs:element name="channel" maxOccurs="unbounded" minOccurs="1">
                                                    <xs:complexType>
                                                      <xs:simpleContent>
                                                        <xs:extension base="xs:string">
                                                          <xs:attribute type="xs:dateTime" name="expiration" use="optional" />
                                                          <xs:attribute type="xs:dateTime" name="start" use="optional" />
                                                        </xs:extension>
                                                      </xs:simpleContent>
                                                    </xs:complexType>
                                                  </xs:element>
                                                </xs:sequence>
                                              </xs:complexType>
                                            </xs:element>
                                          </xs:all>
                                          <xs:attribute type="xs:string" name="id" use="required" />
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="descriptions" maxOccurs="1" minOccurs="0">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="description" minOccurs="0" maxOccurs="unbounded">
                                        <xs:complexType>
                                          <xs:simpleContent>
                                            <xs:extension base="xs:string">
                                              <xs:attribute type="xs:string" name="title" use="optional" />
                                            </xs:extension>
                                          </xs:simpleContent>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="attachments" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="media" minOccurs="1" maxOccurs="unbounded">
                                        <xs:complexType>
                                          <xs:all maxOccurs="1" minOccurs="1">
                                            <xs:element type="xs:string" name="alt" maxOccurs="1" minOccurs="0" />
                                            <xs:element type="xs:string" name="title" maxOccurs="1" minOccurs="0" />
                                            <xs:element type="xs:anyURI" name="file" maxOccurs="1" minOccurs="0" />
                                            <xs:element type="xs:anyURI" name="url" maxOccurs="1" minOccurs="0" />
                                            <xs:element type="xs:string" name="caption" default="" maxOccurs="1" minOccurs="0" />
                                            <xs:element type="xs:string" name="description" maxOccurs="1" minOccurs="0" />
                                            <xs:element type="xs:string" name="publication" maxOccurs="1" minOccurs="0" />
                                          </xs:all>
                                          <xs:attribute type="mediaTypes" name="type" use="required" />
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element type="xs:string" name="visitInformation" minOccurs="0" maxOccurs="1" />
                                <xs:element name="events" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="event" minOccurs="1" maxOccurs="unbounded">
                                        <xs:complexType>
                                          <xs:all>
                                            <xs:element type="xs:string" name="name" minOccurs="1" maxOccurs="1" />
                                            <xs:element type="xs:dateTime" name="start" minOccurs="1" maxOccurs="1" />
                                            <xs:element type="xs:dateTime" name="end" minOccurs="1" maxOccurs="1" />
                                            <xs:element type="xs:string" name="location" minOccurs="0" maxOccurs="1" />
                                            <xs:element type="xs:string" name="summary" minOccurs="0" maxOccurs="1" />
                                          </xs:all>
                                          <xs:attribute type="xs:string" name="type" />
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:all>
                              <xs:attribute type="xs:string" name="lang" use="required" />
                              <!-- <xs:attribute type="xs:ID" name="id" use="optional"/> -->
                              <xs:attribute type="xs:string" name="id" use="optional">
                                <xs:annotation>
                                  <xs:documentation> Is used for machine-identification for the individual offer/languages this needs to be
                                    unique
                                  </xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                  <!-- <xs:attribute type="xs:ID" name="id" use="required"/> -->
                  <xs:attribute type="xs:string" name="id" use="required">
                    <xs:annotation>
                      <xs:documentation> this ID needs to be unique and represents the id from the exporter (this can be the same as
                        referenceId if the exporter is the original creator)
                      </xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:all>
    </xs:complexType>
  </xs:element>
</xs:schema>

      <xs:enumeration value="max_elevator_weight"/>

      <xs:enumeration value="area_property_land"/>
      <xs:enumeration value="property_land_price"/>

      <!-- Pseudo Norm -->
      <xs:enumeration value="area_bwf"/> <!-- Bruttowohnfläche -->
      <xs:enumeration value="area_nwf"/> <!-- Nettowohnfläche -->

      <!-- SIA 416 -->
      <!-- Flächen
      ┌───────────────────────────────────────────────┐
      │                       GF                      │ Geschossfläche
      ├───────────────────────────────┰───────────────┧
      │              NGF              ┃       KF      ┃ Nettogeschossfläche + Konstruktionsfläche
      ├───────────────┬───────┬───────╂───────┬───────┨
      │       NF      │  VF   │  FF   ┃  KFT  │  KFN  ┃ (Nutzfläche + Verkehrsfläche + Funktionsfläche) (FK tragend + KF nicht-tragend)
      ├───────┬───────┼───────┴───────┺━━━━━━━┷━━━━━━━┛
      │  NNF  │  HNF  │ Neben + Hauptnutzfläche
      └───────┴───────┘
      -->
      <xs:enumeration value="area_sia_gf"/> <!-- (Brutto) Geschossfläche -->
      <xs:enumeration value="area_sia_ngf"/> <!-- Nettogeschossfläche -->
      <xs:enumeration value="area_sia_kf"/> <!-- Konstruktionsfläche -->
      <xs:enumeration value="area_sia_nf"/> <!-- Nutzfläche -->
      <xs:enumeration value="area_sia_vf"/> <!-- Verkehrsfläche -->
      <xs:enumeration value="area_sia_ff"/> <!-- Funktionsfläche -->
      <xs:enumeration value="area_sia_kft"/> <!-- Konstruktionsfläche Tragend -->
      <xs:enumeration value="area_sia_kfn"/> <!-- Konstruktionsfläche nicht Tragend -->
      <xs:enumeration value="area_sia_hnf"/> <!-- Hauptnutzfläche -->
      <xs:enumeration value="area_sia_nnf"/> <!-- Nebennutzfläche -->

      <!-- Ausenflächen
      ┌───────────────────────────────────────────────┐
      │                      AGF                      │ Aussen-Geschossfläche
      ├───────────────────────────────┰───────────────┧
      │             ANGF              ┃      AKF      ┃ Aussen-Nettogeschossfläche + Aussen-Konstruktionsfläche
      ├───────────────┬───────┬───────╂───────┬───────┨
      │      ANF      │  AVF  │  AFF  ┃  AKFT │ AKFN  ┃ Aussen-* (Nutzfläche + Verkehrsfläche + Funktionsfläche) (FK tragend + KF nicht-tragend)
      └───────────────┴───────┴───────┺━━━━━━━┷━━━━━━━┛
      -->
      <xs:enumeration value="area_sia_agf"/>
      <xs:enumeration value="area_sia_angf"/>
      <xs:enumeration value="area_sia_akf"/>
      <xs:enumeration value="area_sia_anf"/>
      <xs:enumeration value="area_sia_avf"/>
      <xs:enumeration value="area_sia_aff"/>
      <xs:enumeration value="area_sia_akft"/>
      <xs:enumeration value="area_sia_akfn"/>

      <!-- Volumen
      ┌───────────────────────────────────────────────┐
      │                      GV                       │ Gebäudevolumen
      ├───────────────────────────────┬───────────────┤
      │             ANGV              │      AKV      │ Nettogebäudevolumen + Konstruktions-Volumen
      ├───────────────┬───────┬───────┼───────────────┘
      │      ANV      │  AVV  │  AFV  │   Nutzvolumen + Verkehrsvolumen + Funktionsvolumen
      └───────────────┴───────┴───────┘
      -->
      <xs:enumeration value="volume_sia_gv"/>
      <xs:enumeration value="volume_sia_angv"/>
      <xs:enumeration value="volume_sia_akv"/>
      <xs:enumeration value="volume_sia_anv"/>
      <xs:enumeration value="volume_sia_avv"/>
      <xs:enumeration value="volume_sia_afv"/>

      <!--
      ┌───────────────────────────────────────────────┐
      │                      GSF                      │ Grundstückfläche
      ├───────────────────────────────┬───────────────┤
      │               UF              │      GGF      │ Umgebungsfläche + Gebäudegrundfläche
      ├───────────────┬───────────────┼───────────────┘
      │      BUF      │      UUF      │ Bearbeitete Umgebungsfläche + Unbearbeitete Umgebungsfläche
      └───────────────┴───────────────┘
      -->
      <xs:enumeration value="area_sia_gsf"/>
      <xs:enumeration value="area_sia_uf"/>
      <xs:enumeration value="area_sia_ggf"/>
      <xs:enumeration value="area_sia_buf"/>
      <xs:enumeration value="area_sia_uuf"/>


      <!-- DIN 277 -->
      <!--<xs:enumeration value="area_din_bgf"/>-->
      <!--<xs:enumeration value="area_din_kgf"/>-->
      <!--<xs:enumeration value="area_din_ngf"/>-->
      <!--<xs:enumeration value="area_din_nf"/>-->
      <!--<xs:enumeration value="area_din_tf"/>-->
      <!--<xs:enumeration value="area_din_vf"/>-->

      <xs:enumeration value="volume_gva"/> <!-- Gebäudekubatur GVA (prefered) -->
      <xs:enumeration value="volume_sia"/> <!-- Gebäudekubatur SIA -->


      <xs:enumeration value="gross_premium"/> <!-- Bruttorendite -->
      <xs:enumeration value="utilization_number"/> <!--Ausnützungsziffer-->
      <xs:enumeration value="construction_utilization_number"/> <!--Ausnützungsziffer (bau)-->


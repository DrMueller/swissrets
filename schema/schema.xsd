  <xs:simpleType name="emailAddress">
    <xs:restriction base="xs:string">
      <xs:pattern value="[^@]+@[^\.]+\..+"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="rentalDepositProviders">
    <xs:restriction base="xs:string">
      <xs:enumeration value="adunokaution"/>
      <xs:enumeration value="firstcaution"/>
      <xs:enumeration value="swisscaution"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- MAIN STRUCTURE STARTS HERE -->
  <xs:element name="export">
    <xs:complexType>
      <xs:all>
        <xs:element name="exportInformation" maxOccurs="1" minOccurs="1">
          <xs:complexType>
            <xs:all>
              <xs:element type="xs:string" name="exporter" minOccurs="0" maxOccurs="1"/>
              <xs:element type="xs:dateTime" name="exportDate" minOccurs="0" maxOccurs="1"/>
              <xs:element type="xs:integer" name="exportIteration" default="1" minOccurs="0" maxOccurs="1"/>
            </xs:all>
          </xs:complexType>
        </xs:element>

        <!-- projects -->
        <xs:element name="projects" maxOccurs="1" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="project" maxOccurs="unbounded" minOccurs="1">
                <xs:complexType>
                  <xs:all>
                    <xs:element type="xs:string" name="referenceId" minOccurs="1" maxOccurs="1"/>

                    <xs:element name="units" maxOccurs="1" minOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="unit" maxOccurs="unbounded" minOccurs="1">
                            <xs:complexType>
                              <xs:all>
                                <xs:element type="xs:string" name="referenceId" minOccurs="1" maxOccurs="1"/>
                                <xs:element name="properties" maxOccurs="1" minOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <!-- <xs:element name="propertyRef" maxOccurs="unbounded" minOccurs="1" type="xs:IDREF" /> -->
                                      <xs:element name="propertyRef" maxOccurs="unbounded" minOccurs="1" type="xs:string" />
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:all>
                              <xs:attribute type="xs:ID" name="id" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                  </xs:all>
                  <xs:attribute type="xs:ID" name="id" use="optional"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>

        <!-- properties -->
        <xs:element name="properties" maxOccurs="1" minOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="property" maxOccurs="unbounded" minOccurs="1">
                <xs:complexType>
                  <xs:all>
                    <xs:element type="xs:string" name="zoneTypes" minOccurs="0" maxOccurs="1"/>
                    <xs:element type="xs:string" name="parcelNumbers" minOccurs="0" maxOccurs="1"/>
                    <!-- referenceId can be used to track the original creation id (this should usualy be unique to the property) -->
                    <xs:element type="xs:string" name="referenceId" minOccurs="1" maxOccurs="1"/>
                    <!-- visualReferenceId should only be used for presentational purposes publishers tend to present the referenceId if this one is missing -->
                    <xs:element type="xs:string" name="visualReferenceId" minOccurs="0" maxOccurs="1"/>
                    <xs:element type="xs:string" name="type" minOccurs="1" maxOccurs="1"/>
                    <xs:element type="availability" name="availability" minOccurs="1" maxOccurs="1"/>
                    <xs:element type="xs:dateTime" name="start" minOccurs="0" maxOccurs="1"/>
                    <xs:element type="currencyCodeType" name="priceCurrency" minOccurs="1" maxOccurs="1"/>
                    <xs:element type="price" name="price" minOccurs="0" maxOccurs="1"/>
                    <xs:element type="priceRange" name="priceRange" minOccurs="0" maxOccurs="1"/>
                    <xs:element type="price" name="netPrice" minOccurs="0" maxOccurs="1"/>
                    <xs:element type="price" name="grossPrice" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="extraCosts" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="extraCost" minOccurs="1" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:short">
                                  <xs:attribute type="xs:string" name="key" use="optional"/>
                                  <xs:attribute type="xs:byte" name="frequency" use="optional"/>
                                  <xs:attribute type="xs:string" name="timesegment" use="optional"/>
                                  <xs:attribute type="xs:string" name="propertysegment" use="optional"/>
                                  <xs:attribute type="xs:string" name="type" use="optional"/>
                                  <xs:attribute type="xs:string" name="title" use="optional"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="integratedOffers" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="integratedOffer" minOccurs="1" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:unsignedInt">
                                  <xs:attribute type="integratedOfferType" name="type" use="optional"/>
                                  <xs:attribute type="xs:byte" name="frequency" use="optional"/>
                                  <xs:attribute type="xs:string" name="timesegment" use="optional"/>
                                  <xs:attribute type="xs:string" name="propertysegment" use="optional"/>
                                  <xs:attribute type="xs:byte" name="inclusive" use="optional"/> <!-- determines if is added to offer-price (inclusive = 0) or appart of it (inclusive = 1) -->
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="address" type="address" minOccurs="1" maxOccurs="1"/>

                    <xs:element name="softwareInformation" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                          <xs:sequence>
                            <xs:element type="xs:string" name="lastUser" minOccurs="0" maxOccurs="1"/>
                            <xs:element type="xs:dateTime" name="lastUpdate" minOccurs="1" maxOccurs="1"/>
                            <xs:element type="xs:byte" name="updateIteration" minOccurs="0" maxOccurs="1"/>
                            <xs:element type="xs:dateTime" name="creation" minOccurs="0" maxOccurs="1"/>
                          </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                    <xs:element name="features" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="feature" type="featureTypes" maxOccurs="unbounded" minOccurs="1"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="numericValues" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="value" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="numericValuekeys" name="key" use="required"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="categories" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="categoryTypes" name="category" minOccurs="1" maxOccurs="3"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="utilities" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="utilityTypes" name="utility" minOccurs="1" maxOccurs="3"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="relations" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="relation" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="relationkeys" name="key" use="required"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="seller" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:all>
                          <xs:element name="organization" minOccurs="0" maxOccurs="1">
                            <xs:complexType>
                              <xs:all>
                                <xs:element name="address" type="address" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="xs:string" name="legalName" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="xs:string" name="brand" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="emailAddress" name="email" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="emailAddress" name="emailRem" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="xs:string" name="fax" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="xs:string" name="phone" minOccurs="0" maxOccurs="1"/>
                                <xs:element name="website" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:simpleContent>
                                      <xs:extension base="xs:anyURI">
                                      <xs:attribute type="xs:string" name="title"/>
                                      <xs:attribute type="xs:string" name="label"/>
                                      </xs:extension>
                                    </xs:simpleContent>
                                  </xs:complexType>
                                </xs:element>
                              </xs:all>
                              <xs:attribute type="xs:string" name="id" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                          <xs:element type="person" name="viewPerson" minOccurs="0" maxOccurs="1"/>
                          <xs:element type="person" name="visitPerson" minOccurs="0" maxOccurs="1"/>
                          <xs:element type="person" name="inquiryPerson" minOccurs="0" maxOccurs="1"/>
                        </xs:all>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="rentalDepositProviders" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="rentalDepositProvider" minOccurs="1" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:all>
                                <xs:element type="xs:string" name="calculation" minOccurs="0" maxOccurs="1"/>
                              </xs:all>
                              <xs:attribute type="rentalDepositProviders" name="provider" use="required"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="offers" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="offer" minOccurs="1" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:all>
                                <xs:element type="xs:string" name="name" minOccurs="1" maxOccurs="1"/>
                                <xs:element type="xs:string" name="excerpt" minOccurs="0" maxOccurs="1"/>

                                <xs:element name="tags" maxOccurs="1" minOccurs="0">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="tag" minOccurs="0" maxOccurs="unbounded"/>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>

                                <xs:element name="urls" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element type="fullUrl" name="url" minOccurs="0" maxOccurs="unbounded"/>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>

                                <xs:element name="publishers" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="publisher" minOccurs="1" maxOccurs="unbounded">
                                        <xs:complexType>
                                          <xs:all>
                                            <xs:element name="options" minOccurs="0" maxOccurs="1">
                                              <xs:complexType mixed="true">
                                                <xs:sequence>
                                                  <xs:element name="option" maxOccurs="unbounded" minOccurs="1">
                                                  <xs:complexType>
                                                  <xs:simpleContent>
                                                  <xs:extension base="xs:string">
                                                  <xs:attribute type="xs:string" name="key" use="optional"/>
                                                  </xs:extension>
                                                  </xs:simpleContent>
                                                  </xs:complexType>
                                                  </xs:element>
                                                </xs:sequence>
                                              </xs:complexType>
                                            </xs:element>
                                            <xs:element name="channels" minOccurs="0" maxOccurs="1">
                                              <xs:complexType mixed="true">
                                                <xs:sequence>
                                                  <xs:element name="channel" maxOccurs="unbounded" minOccurs="1">
                                                  <xs:complexType>
                                                  <xs:simpleContent>
                                                  <xs:extension base="xs:string">
                                                    <xs:attribute type="xs:dateTime" name="expiration" use="optional"/>
                                                  </xs:extension>
                                                  </xs:simpleContent>
                                                  </xs:complexType>
                                                  </xs:element>
                                                </xs:sequence>
                                              </xs:complexType>
                                            </xs:element>
                                          </xs:all>
                                          <xs:attribute type="xs:string" name="id" use="required"/>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>

                                <xs:element name="descriptions" maxOccurs="1" minOccurs="0">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="description" minOccurs="0" maxOccurs="unbounded">
                                        <xs:complexType>
                                          <xs:simpleContent>
                                            <xs:extension base="xs:string">
                                              <xs:attribute type="xs:string" name="title" use="optional"/>
                                            </xs:extension>
                                          </xs:simpleContent>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>

                                <xs:element name="attachments" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="media" minOccurs="1" maxOccurs="unbounded">
                                        <xs:complexType>
                                          <xs:all maxOccurs="1" minOccurs="1">
                                            <xs:element type="xs:string" name="alt" maxOccurs="1"
                                              minOccurs="0"/>
                                            <xs:element type="xs:string" name="title" maxOccurs="1"
                                              minOccurs="0"/>
                                            <xs:element type="xs:anyURI" name="file" maxOccurs="1"
                                              minOccurs="0"/>
                                            <xs:element type="xs:anyURI" name="url" maxOccurs="1"
                                              minOccurs="0"/>
                                            <xs:element type="xs:string" name="caption" default=""
                                              maxOccurs="1" minOccurs="0"/>
                                            <xs:element type="xs:string" name="description"
                                              maxOccurs="1" minOccurs="0"/>
                                              <xs:element type="xs:string" name="publication" maxOccurs="1"
                                              minOccurs="0"/>
                                          </xs:all>
                                          <xs:attribute type="mediaTypes" name="type" use="required"
                                          />
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>

                                <xs:element type="xs:string" name="visitInformation" minOccurs="0" maxOccurs="1"/>

                                <xs:element name="events" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="event" minOccurs="1" maxOccurs="unbounded">
                                        <xs:complexType>
                                          <xs:all>
                                            <xs:element type="xs:string" name="name" minOccurs="1"
                                              maxOccurs="1"/>
                                            <xs:element type="xs:dateTime" name="start"
                                              minOccurs="1" maxOccurs="1"/>
                                            <xs:element type="xs:dateTime" name="end" minOccurs="1"
                                              maxOccurs="1"/>
                                            <xs:element type="xs:string" name="location"
                                              minOccurs="0" maxOccurs="1"/>
                                            <xs:element type="xs:string" name="summary"
                                              minOccurs="0" maxOccurs="1"/>
                                          </xs:all>
                                          <xs:attribute type="xs:string" name="type"/>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>

                              </xs:all>
                              <xs:attribute type="xs:string" name="lang" use="required"/>
                              <!-- offer->id is used for machine-identification for the individual offer/languages this needs to be unique -->
                              <!-- <xs:attribute type="xs:ID" name="id" use="optional"/> -->
                              <xs:attribute type="xs:string" name="id" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:all>
                  <!-- this ID needs to be unique and represents the id from the exporter (this can be the same as referenceId if the exporter is the original creator) -->
                  <!-- <xs:attribute type="xs:ID" name="id" use="required"/> -->
                  <xs:attribute type="xs:string" name="id" use="required"/>
                  <xs:attribute type="xs:integer" name="layer" use="optional"/>
                  <xs:attribute type="xs:string" name="parent" use="optional"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:all>
    </xs:complexType>
  </xs:element>
</xs:schema>

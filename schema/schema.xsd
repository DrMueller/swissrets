      <xs:enumeration value="boat-landing-stage" />
      <!--'Boot Stegplatz','Boat landing stage','Place extérieure à bâteaux','Attracco barca esteriore', -->
      <xs:enumeration value="horse-box" />
      <!--'Stallboxe','Horse box','Box d\'écurie','Box in stalla', -->
      <xs:enumeration value="boat-mooring" />
      <!--'Boot Bojenplatz','Boat mooring','Place à bâteau balisée','Attracco barca (boa)', -->
      <xs:enumeration value="cellar-compartment" />
      <!--'Kellerabteil','Cellar compartment','Cave','Scomparto cantina', -->
      <xs:enumeration value="attic-compartment" />
      <!--'Estrichabteil','Attic compartment','Grenier','Scomparto soffitta', -->
